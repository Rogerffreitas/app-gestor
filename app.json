{
    "expo": {
        "name": "Gestor Ca√ßambas e Equipamentos",
        "slug": "app-gestor",
        "version": "1.0.4-alfa",
        "orientation": "portrait",
        "icon": "./assets/image/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/image/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff",
            "imageWidth": 200
        },
        "plugins": [
            "expo-build-properties",
            "@morrowdigital/watermelondb-expo-plugin",
            "@react-native-community/datetimepicker",
            "expo-secure-store"
        ],
        "extra": {
            "eas": {
                "projectId": "1fac0061-61da-44e0-bb4a-02aa69e07abb"
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/image/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.rogerffreitas.app-gestor",
            "runtimeVersion": "1.0.1-beta",
            "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"],
            "versionCode": 2
        },
        "ios": {
            "bundleIdentifier": "com.rogerfreitas.appgestor"
        }
    }
